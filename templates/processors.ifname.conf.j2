# Add a tag of the network interface name looked up over SNMP by interface number
[[processors.ifname]]
  ## Name of tag holding the interface number
  {{ '#' if telegraf_processors_ifname_tag is not defined }}tag = "{{ telegraf_processors_ifname_tag|d() }}"

  ## Name of output tag where service name will be added
  {{ '#' if telegraf_processors_ifname_dest is not defined }}dest = "{{ telegraf_processors_ifname_dest|d() }}"

  ## Name of tag of the SNMP agent to request the interface name from
  {{ '#' if telegraf_processors_ifname_agent is not defined }}agent = "{{ telegraf_processors_ifname_agent|d() }}"

  ## Timeout for each request.
  {{ '#' if telegraf_processors_ifname_timeout is not defined }}timeout = "{{ telegraf_processors_ifname_timeout|d() }}"

  ## SNMP version; can be 1, 2, or 3.
  {{ '#' if telegraf_processors_ifname_version is not defined }}version = {{ telegraf_processors_ifname_version|d() }}

  ## SNMP community string.
  {{ '#' if telegraf_processors_ifname_community is not defined }}community = "{{ telegraf_processors_ifname_community|d() }}"

  ## Number of retries to attempt.
  {{ '#' if telegraf_processors_ifname_retries is not defined }}retries = {{ telegraf_processors_ifname_retries|d() }}

  ## The GETBULK max-repetitions parameter.
  {{ '#' if telegraf_processors_ifname_max_repetitions is not defined }}max_repetitions = {{ telegraf_processors_ifname_max_repetitions|d() }}

  ## SNMPv3 authentication and encryption options.
  ##
  ## Security Name.
  {{ '#' if telegraf_processors_ifname_sec_name is not defined }}sec_name = "{{ telegraf_processors_ifname_sec_name|d() }}"
  ## Authentication protocol; one of "MD5", "SHA", or "".
  {{ '#' if telegraf_processors_ifname_auth_protocol is not defined }}auth_protocol = "{{ telegraf_processors_ifname_auth_protocol|d() }}"
  ## Authentication password.
  {{ '#' if telegraf_processors_ifname_auth_password is not defined }}auth_password = "{{ telegraf_processors_ifname_auth_password|d() }}"
  ## Security Level; one of "noAuthNoPriv", "authNoPriv", or "authPriv".
  {{ '#' if telegraf_processors_ifname_sec_level is not defined }}sec_level = "{{ telegraf_processors_ifname_sec_level|d() }}"
  ## Context Name.
  {{ '#' if telegraf_processors_ifname_context_name is not defined }}context_name = "{{ telegraf_processors_ifname_context_name|d() }}"
  ## Privacy protocol used for encrypted messages; one of "DES", "AES" or "".
  {{ '#' if telegraf_processors_ifname_priv_protocol is not defined }}priv_protocol = "{{ telegraf_processors_ifname_priv_protocol|d() }}"
  ## Privacy password used for encrypted messages.
  {{ '#' if telegraf_processors_ifname_priv_password is not defined }}priv_password = "{{ telegraf_processors_ifname_priv_password|d() }}"

  ## max_parallel_lookups is the maximum number of SNMP requests to
  ## make at the same time.
  {{ '#' if telegraf_processors_ifname_max_parallel_lookups is not defined }}max_parallel_lookups = {{ telegraf_processors_ifname_max_parallel_lookups|d() }}

  ## ordered controls whether or not the metrics need to stay in the
  ## same order this plugin received them in. If false, this plugin
  ## may change the order when data is cached.  If you need metrics to
  ## stay in order set this to true.  keeping the metrics ordered may
  ## be slightly slower
  {{ '#' if telegraf_processors_ifname_ordered is not defined }}ordered = {{ telegraf_processors_ifname_ordered|d()|bool|lower }}

  ## cache_ttl is the amount of time interface names are cached for a
  ## given agent.  After this period elapses if names are needed they
  ## will be retrieved again.
  {{ '#' if telegraf_processors_ifname_cache_ttl is not defined }}cache_ttl = "{{ telegraf_processors_ifname_cache_ttl|d() }}"
